{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true,
        "experimentalObjectRestSpread": true,
    },
    "rules": {
        "no-unused-vars":1,
        "quotes": [2, "single", "avoid-escape"],
        "no-use-before-define": [2,"nofunc"],
        "max-statements":[1, 20],
        "max-depth":[1, 2],
        "complexity":[1, 5],
        "max-params":[1, 5],
        "max-nested-callbacks":0,
        "no-switch":[0],
        "consistent-this":2,
        "no-extra-bind":1,
        "curly":1,
        "dot-notation":1,
        "no-eq-null":2,
        "no-implied-eval":2,
        "no-loop-func":1,
        "no-multi-spaces":1,
        "no-multi-str":2,
        "no-new-wrappers":1,
        "no-self-compare":1,
        "vars-on-top":1,
        "yoda":2,
        "eol-last":2,
        "func-names":1,
        "no-array-constructor":1,
        "no-nested-ternary":1,
        "no-new-object":1,
        "no-trailing-spaces":2,
        "no-console":0,
        "no-undef":0
    }
}
